; file: /usr/local/etc/namedb/primary/lab.net-forward.db
$TTL  1h
@       IN      SOA     dns.lab.net. root.dns.lab.net. (
                        ;; [YYYYMMDDnn] (update date + number)
                        2025100501; Serial
                        1h;   Refresh
                        15m;    Retry
                        1w;   Expiry
                        1d;   Minimum
)

; Name server
      IN  NS  dns.lab.net.
      IN  A   10.0.1.2

; Servers
dns   IN  A 10.0.1.2
